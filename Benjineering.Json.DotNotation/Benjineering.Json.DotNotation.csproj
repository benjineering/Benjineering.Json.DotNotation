<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<PropertyGroup>
		<Authors>Ben Williams</Authors>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageDescription>Helpers and extension methods for querying System.Text.Json.JsonElement objects via dot notation.</PackageDescription>
		<RepositoryUrl>https://github.com/benjineering/Benjineering.Json.DotNotation</RepositoryUrl>
	</PropertyGroup>

	<ItemGroup>
		<None Include="../LICENSE.txt" Pack="true" PackagePath="LICENSE.txt"/>
		<None Include="../README.md" Pack="true" PackagePath="README.md"/>
	</ItemGroup>

</Project>
